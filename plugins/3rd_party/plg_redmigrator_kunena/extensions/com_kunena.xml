<?xml version="1.0" ?>
<!DOCTYPE redmigrator>
<redmigrator>
	<name>Kunena for Joomla! 2.5!</name>

	<!-- The folders to copy to the new site. -->
	<folders>
		<folder>administrator/components/com_kunena</folder>
		<folder>components/com_kunena</folder>
		<folder>media/kunena</folder>
	</folders>

	<!-- The tables to copy to the new site. -->
	<tables> 		
		<table name="announcement" title="KUNENA Announcement" tbl_key="id" source="fb_announcement" destination="kunena_announcement" type="kunena" class="RedMigratorComKunenaAnnouncement">fb_announcement</table>

		<table name="attachment" title="KUNENA Attachments" tbl_key="id" source="fb_attachments" destination="kunena_attachments" type="kunena" class="RedMigratorComKunenaAttachment">fb_attachments</table>

		<table name="category" title="KUNENA Categories" tbl_key="id" source="fb_categories" destination="kunena_categories" type="kunena" class="RedMigratorComKunenaCategory">fb_categories</table>

		<table name="message" title="KUNENA Message" tbl_key="id" source="fb_messages" destination="kunena_messages" type="kunena" class="RedMigratorComKunenaMessage">fb_messages</table>
		
		<table name="message_text" title="KUNENA Message Text" tbl_key="mesid" source="fb_messages_text" destination="kunena_messages_text" type="kunena" class="RedMigratorComKunenaMessageText">fb_messages_text</table>
		
		<table name="rank" title="KUNENA Ranks" tbl_key="rank_id" source="fb_ranks" destination="kunena_ranks" type="kunena" class="RedMigratorComKunenaRank">fb_ranks</table>

		<table name="session" title="KUNENA Sessions" tbl_key="id" source="fb_sessions" destination="kunena_sessions" type="kunena" class="RedMigratorComKunenaSession">fb_sessions</table>

		<table name="smiley" title="KUNENA Smileys" tbl_key="id" source="fb_smileys" destination="kunena_smileys" type="kunena" class="RedMigratorComKunenaSmiley">fb_smileys</table>

		<table name="user" title="KUNENA Users" tbl_key="userid" source="fb_users" destination="kunena_users" type="kunena" class="RedMigratorComKunenaUser">fb_users</table>

		<table name="version" title="KUNENA Version" tbl_key="id" source="fb_version" destination="kunena_version" type="kunena" class="RedMigratorComKunenaVersion">fb_version</table>
	</tables>

	<!-- List of Kunena extensions. -->
	<package>
		<extension>
			<name>plg_system_kunena</name>
		</extension>
		<extension>
			<name>mod_kunenalatest</name>
		</extension>
		<extension>
			<name>mod_kunenalogin</name>
		</extension>
		<extension>
			<name>mod_kunenasearch</name>
		</extension>
		<extension>
			<name>mod_kunenastats</name>
		</extension>
		<extension>
			<name>plg_community_kunenagroups</name>
		</extension>
		<extension>
			<name>plg_community_kunenamenu</name>
		</extension>
		<extension>
			<name>plg_community_mykunena</name>
		</extension>
		<extension>
			<name>plg_jxfinder_kunena</name>
		</extension>
		<extension>
			<name>plg_system_kunenadiscuss</name>
		</extension>
		<extension>
			<name>plg_search_kunenasearch</name>
		</extension>
		<extension>
			<name>com_kunenaimporter</name>
		</extension>
	</package>
</redmigrator>
